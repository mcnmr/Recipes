		Coal5.setItemMeta(Coal5Meta);
		inv5.setItem(22, Coal5);
		ItemStack Coal6 = new ItemStack(Material.STONE);
		ItemMeta Coal6Meta = Coal6.getItemMeta();
		Coal6Meta.setDisplayName("Stone");
		Coal6.setItemMeta(Coal6Meta);
		inv5.setItem(28, Coal6);
		ItemStack Coal7 = new ItemStack(Material.STONE);
		ItemMeta Coal7Meta = Coal7.getItemMeta();
		Coal7Meta.setDisplayName("Stone");
		Coal7.setItemMeta(Coal7Meta);
		inv5.setItem(30, Coal7);
		ItemStack Back = new ItemStack(Material.REDSTONE_BLOCK);
		ItemMeta BackMeta = Back.getItemMeta();
		BackMeta.setDisplayName(ChatColor.BLUE + "Go Back");
		Back.setItemMeta(BackMeta);
		inv5.setItem(44, Back);
		ItemStack Disc13 = new ItemStack(Material.CHAINMAIL_LEGGINGS);
		ItemMeta Disc13Meta = Disc13.getItemMeta();
		Disc13Meta.setDisplayName(ChatColor.AQUA + "Chain Leggings");
		Disc13.setItemMeta(Disc13Meta);
		inv5.setItem(24, Disc13);
		commander.openInventory(inv5);
	}	
	private void openGui20(Player commander) {

		Inventory inv5 = Bukkit.createInventory(null, 45, ChatColor.DARK_BLUE
				+ "Recipe for Chain Boots"); 
		
		ItemStack Coal2 = new ItemStack(Material.STONE);
		ItemMeta Coal2Meta = Coal2.getItemMeta();
		Coal2Meta.setDisplayName("Stone");
		Coal2.setItemMeta(Coal2Meta);
		inv5.setItem(10, Coal2);
		ItemStack Coal3 = new ItemStack(Material.STONE);
		ItemMeta Coal3Meta = Coal3.getItemMeta();
		Coal3Meta.setDisplayName("Stone");
		Coal3.setItemMeta(Coal3Meta);
		inv5.setItem(12, Coal3);
		ItemStack Coal4 = new ItemStack(Material.STONE);
		ItemMeta Coal4Meta = Coal4.getItemMeta();
		Coal4Meta.setDisplayName("Stone");
		Coal4.setItemMeta(Coal4Meta);
		inv5.setItem(19, Coal4);
		ItemStack Sugar = new ItemStack(Material.STONE);
		ItemMeta SugarMeta = Sugar.getItemMeta();
		SugarMeta.setDisplayName("Stone");
		Sugar.setItemMeta(SugarMeta);
		inv5.setItem(21, Sugar);
		ItemStack Back = new ItemStack(Material.REDSTONE_BLOCK);
		ItemMeta BackMeta = Back.getItemMeta();
		BackMeta.setDisplayName(ChatColor.BLUE + "Go Back");
		Back.setItemMeta(BackMeta);
		inv5.setItem(44, Back);
		ItemStack Disc13 = new ItemStack(Material.CHAINMAIL_BOOTS);
		ItemMeta Disc13Meta = Disc13.getItemMeta();
		Disc13Meta.setDisplayName(ChatColor.AQUA + "Chain Boots");
		Disc13.setItemMeta(Disc13Meta);
		inv5.setItem(24, Disc13);
		commander.openInventory(inv5);
	}	
//Stopping at Saddle 
