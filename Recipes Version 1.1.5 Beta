		disk7.shape(new String[] { "$$$", "$6$", "$$$" })
				.setIngredient('$', Material.COAL)
				.setIngredient('6', Material.BED);
		getServer().addRecipe(disk7);
		}
		if (config.getBoolean("Disc stal") == true);
		{
		ShapedRecipe disk8 = new ShapedRecipe(new ItemStack(Material.RECORD_8,
				1));
		disk8.shape(new String[] { "$$$", "$7$", "$$$" })
				.setIngredient('$', Material.COAL)
				.setIngredient('7', Material.FLINT);
		getServer().addRecipe(disk8);
		}
		if (config.getBoolean("Disc strad") == true);
		{
		ShapedRecipe disk9 = new ShapedRecipe(new ItemStack(Material.RECORD_9,
				1));
		disk9.shape(new String[] { "$$$", "$8$", "$$$" })
				.setIngredient('$', Material.COAL)
				.setIngredient('8', Material.IRON_INGOT);
		getServer().addRecipe(disk9);
		}
		if (config.getBoolean("Disc ward") == true);
		{
		ShapedRecipe disk10 = new ShapedRecipe(new ItemStack(Material.RECORD_10, 1));
		disk10.shape(new String[] { "$$$", "$9$", "$$$" })
				.setIngredient('$', Material.COAL)
				.setIngredient('9', Material.MOSSY_COBBLESTONE);
		getServer().addRecipe(disk10);
		}
		if (config.getBoolean("Disc 11") == true);
		{
		ShapedRecipe disk11 = new ShapedRecipe(new ItemStack(Material.RECORD_11, 1));
		disk11.shape(new String[] { "$$$", "$!$", "$$$" })
				.setIngredient('$', Material.COAL)
				.setIngredient('!', Material.COAL_BLOCK);
		getServer().addRecipe(disk11);
		}
		if (config.getBoolean("Disc wait") == true);
		{
		ShapedRecipe disk12 = new ShapedRecipe(new ItemStack(Material.RECORD_12, 1));
		disk12.shape(new String[] { "$$$", "$@$", "$$$" })
				.setIngredient('$', Material.COAL)
				.setIngredient('@', Material.ICE);
		getServer().addRecipe(disk12);
		}
		if (config.getBoolean("Gold Horse Armor") == true);
		{
		ShapedRecipe Ghorse = new ShapedRecipe(new ItemStack(Material.GOLD_BARDING, 1));
		Ghorse.shape(new String[] { " ##", "##&", "#&&" })
				.setIngredient('#', Material.GOLD_INGOT)
				.setIngredient('&', Material.LEATHER);
		getServer().addRecipe(Ghorse);
		}
		if (config.getBoolean("Iron Horse Armor") == true);
		{
		ShapedRecipe Ihorse = new ShapedRecipe(new ItemStack(Material.IRON_BARDING, 1));
		Ihorse.shape(new String[] { " ##", "##&", "#&&" })
				.setIngredient('#', Material.IRON_INGOT)
				.setIngredient('&', Material.LEATHER);
		getServer().addRecipe(Ihorse);
		}
		if (config.getBoolean("Diamond Horse Armor") == true);
		{
		ShapedRecipe Dhorse = new ShapedRecipe(new ItemStack(Material.DIAMOND_BARDING, 1));
		Dhorse.shape(new String[] { " ##", "##&", "#&&" })
				.setIngredient('#', Material.DIAMOND)
				.setIngredient('&', Material.LEATHER);
		getServer().addRecipe(Dhorse);
		}
		if (config.getBoolean("Disc chirp") == true);
		ShapedRecipe Saddle = new ShapedRecipe(new ItemStack(Material.SADDLE, 1));
		Saddle.shape(new String[] { "&&&", "&&&", "&&&" })
				.setIngredient('&', Material.LEATHER);
		getServer().addRecipe(Saddle);
		if (config.getBoolean("Disc chirp") == true);
		ShapedRecipe Chainhelm = new ShapedRecipe(new ItemStack(Material.CHAINMAIL_HELMET, 1));
		Chainhelm.shape(new String[] { "&&&", "& &", "   " })
				.setIngredient('&',
				Material.STONE);
		getServer().addRecipe(Chainhelm);
		if (config.getBoolean("Disc chirp") == true);
		ShapedRecipe Chainchest = new ShapedRecipe(new ItemStack(Material.CHAINMAIL_CHESTPLATE, 1));
		Chainchest.shape(new String[] { "& &", "&&&", "&&&" })
				.setIngredient('&',
				Material.STONE);
		getServer().addRecipe(Chainchest);
		if (config.getBoolean("Disc chirp") == true);
		ShapedRecipe Chainlegg = new ShapedRecipe(new ItemStack(Material.CHAINMAIL_LEGGINGS, 1));
		Chainlegg.shape(new String[] { "&&&", "& &", "& &" })
				.setIngredient('&',
				Material.STONE);
		getServer().addRecipe(Chainlegg);
		if (config.getBoolean("Disc chirp") == true);
		ShapedRecipe Chainboot = new ShapedRecipe(new ItemStack(Material.CHAINMAIL_BOOTS, 1));
		Chainboot.shape(new String[] { "   ", "& &", "& &" })
				.setIngredient('&',
				Material.STONE);
		getServer().addRecipe(Chainboot);
		if (config.getBoolean("Disc chirp") == true);
		ShapedRecipe Chainboot2 = new ShapedRecipe(new ItemStack(Material.CHAINMAIL_BOOTS, 1));
		Chainboot2.shape(new String[] { "& &", "& &", "   " })
				.setIngredient('&',
				Material.STONE);
		getServer().addRecipe(Chainboot2);
		if (config.getBoolean("Disc chirp") == true);
	
		PluginDescriptionFile pdfFile = this.getDescription();
		this.logger.info(pdfFile.getName() + " Has been Enabled!");

			if(!(new File(getDataFolder(), "RESET.FILE")).exists()){
				try
				{
					getConfig().set("Disc 13", true);
					getConfig().set("Disc cat", true);
					getConfig().set("Disc blocks", true);
					getConfig().set("Disc chirp", true);
					getConfig().set("Disc far", true);
					getConfig().set("Disc mall", true);
					getConfig().set("Disc mellohi", true);
					getConfig().set("Disc stal", true);
					getConfig().set("Disc strad", true);
					getConfig().set("Disc ward", true);
					getConfig().set("Disc 11", true);
					getConfig().set("Disc wait", true);
					getConfig().set("Gold Horse Armor", true);
					getConfig().set("Iron Horse Armor", true);
					getConfig().set("Diamond Horse Armor", true);
					getConfig().set("Saddle", true);
					getConfig().set("Chain Helmet", true);
					getConfig().set("Chain Chestplate", true);
					getConfig().set("Chain Leggings", true);
					getConfig().set("Chain Boots", true);
					
					getLogger().info("Creating New Config");
					new File(getDataFolder(), "RESET.FILE").createNewFile();
				}
				catch (IOException e)
				{
					e.printStackTrace();
				}
				saveConfig();
			}
	}

	private void openGUI(Player commander) {
		Inventory inv = Bukkit.createInventory(null, 27, ChatColor.DARK_BLUE
				+ "Recipes list" + " - " + "Click for info!");
		int recipeIndex = 0;
		
		if (getConfig().getBoolean("Disc 13")== true){
			ItemStack Disc13 = new ItemStack(Material.GOLD_RECORD);
			ItemMeta Disc13Meta = Disc13.getItemMeta();
			Disc13Meta.setDisplayName(ChatColor.GOLD + "/rec 13");
			Disc13.setItemMeta(Disc13Meta);
			inv.setItem(recipeIndex++, Disc13);
		}
		if (getConfig().getBoolean("Disc cat")== true){
			ItemStack Disccat = new ItemStack(Material.GREEN_RECORD);
			ItemMeta DisccatMeta = Disccat.getItemMeta();
			DisccatMeta.setDisplayName(ChatColor.GOLD + "/rec cat");
			Disccat.setItemMeta(DisccatMeta);
			inv.setItem(recipeIndex++, Disccat);
		}
		if (getConfig().getBoolean("Disc blocks")== true){
			ItemStack DiscBlocks = new ItemStack(Material.RECORD_3);
			ItemMeta DiscBlocksMeta = DiscBlocks.getItemMeta();
			DiscBlocksMeta.setDisplayName(ChatColor.GOLD + "/rec blocks");
			DiscBlocks.setItemMeta(DiscBlocksMeta);
			inv.setItem(recipeIndex++, DiscBlocks);
		}
		if (getConfig().getBoolean("Disc chirp")== true){
			ItemStack DiscChirp = new ItemStack(Material.RECORD_4);
			ItemMeta DiscChirpMeta = DiscChirp.getItemMeta();
			DiscChirpMeta.setDisplayName(ChatColor.GOLD + "/rec chirp");
			DiscChirp.setItemMeta(DiscChirpMeta);
			inv.setItem(recipeIndex++, DiscChirp);
		}
		if (getConfig().getBoolean("Disc far")== true){
			ItemStack Discfar = new ItemStack(Material.RECORD_5);
			ItemMeta DiscfarMeta = Discfar.getItemMeta();
			DiscfarMeta.setDisplayName(ChatColor.GOLD + "/rec far");
			Discfar.setItemMeta(DiscfarMeta);
			inv.setItem(recipeIndex++, Discfar);
		}
		if (getConfig().getBoolean("Disc mall")== true){
			ItemStack Discmall = new ItemStack(Material.RECORD_6);
			ItemMeta DiscmallMeta = Discmall.getItemMeta();
			DiscmallMeta.setDisplayName(ChatColor.GOLD + "/rec mall");
			Discmall.setItemMeta(DiscmallMeta);
			inv.setItem(recipeIndex++, Discmall);
		}
		if (getConfig().getBoolean("Disc 11")== true){
			ItemStack Discmellohi = new ItemStack(Material.RECORD_7);
			ItemMeta DiscmellohiMeta = Discmellohi.getItemMeta();
			DiscmellohiMeta.setDisplayName(ChatColor.GOLD + "/rec mellohi");
			Discmellohi.setItemMeta(DiscmellohiMeta);
			inv.setItem(recipeIndex++, Discmellohi);
		}
		if (getConfig().getBoolean("Disc ward")== true){
			ItemStack Disc11 = new ItemStack(Material.RECORD_11);
			ItemMeta Disc11Meta = Disc11.getItemMeta();
			Disc11Meta.setDisplayName(ChatColor.GOLD + "/rec 11");
			Disc11.setItemMeta(Disc11Meta);
			inv.setItem(recipeIndex++, Disc11);
		}
		if (getConfig().getBoolean("Disc mellohi")== true){
			ItemStack Discstal = new ItemStack(Material.RECORD_8);
			ItemMeta DiscstalMeta = Discstal.getItemMeta();
			DiscstalMeta.setDisplayName(ChatColor.GOLD + "/rec stal");
			Discstal.setItemMeta(DiscstalMeta);
			inv.setItem(recipeIndex++, Discstal);
		}
		if (getConfig().getBoolean("Disc stal")== true){
			ItemStack Discstrad = new ItemStack(Material.RECORD_9);
			ItemMeta DiscstradpMeta = Discstrad.getItemMeta();
			DiscstradpMeta.setDisplayName(ChatColor.GOLD + "/rec strad");
			Discstrad.setItemMeta(DiscstradpMeta);
			inv.setItem(recipeIndex++, Discstrad);
		}
		if (getConfig().getBoolean("Disc strad")== true){
			ItemStack Discward = new ItemStack(Material.RECORD_10);
			ItemMeta DiscwardMeta = Discward.getItemMeta();
			DiscwardMeta.setDisplayName(ChatColor.GOLD + "/rec ward");
			Discward.setItemMeta(DiscwardMeta);
			inv.setItem(recipeIndex++, Discward);
		}
		if (getConfig().getBoolean("Disc wait")== true){
			ItemStack Discwait = new ItemStack(Material.RECORD_12);
			ItemMeta DiscwaitMeta = Discwait.getItemMeta();
			DiscwaitMeta.setDisplayName(ChatColor.GOLD + "/rec wait");
			Discwait.setItemMeta(DiscwaitMeta);
			inv.setItem(recipeIndex++, Discwait);
		}
		if (getConfig().getBoolean("Gold Horse Armor")== true){
			ItemStack Ghorse = new ItemStack(Material.GOLD_BARDING);
			ItemMeta GhorseMeta = Ghorse.getItemMeta();
			GhorseMeta.setDisplayName(ChatColor.GOLD + "/rec goldhorse");
			Ghorse.setItemMeta(GhorseMeta);
			inv.setItem(recipeIndex++, Ghorse);
		}
		if (getConfig().getBoolean("Iron Horse Armor")== true){
			ItemStack ihorse = new ItemStack(Material.IRON_BARDING);
			ItemMeta ihorseMeta = ihorse.getItemMeta();
			ihorseMeta.setDisplayName(ChatColor.GOLD + "/rec ironhorse");
			ihorse.setItemMeta(ihorseMeta);
			inv.setItem(recipeIndex++, ihorse);
		}
		if (getConfig().getBoolean("Diamond Horse Armor")== true){
			ItemStack Dhorse = new ItemStack(Material.DIAMOND_BARDING);
			ItemMeta DhorseMeta = Dhorse.getItemMeta();
			DhorseMeta.setDisplayName(ChatColor.GOLD + "/rec diamondhorse");
			Dhorse.setItemMeta(DhorseMeta);
			inv.setItem(recipeIndex++, Dhorse);
		}
		if (getConfig().getBoolean("Saddle")== true){
			ItemStack Saddle = new ItemStack(Material.SADDLE);
			ItemMeta SaddleMeta = Saddle.getItemMeta();
			SaddleMeta.setDisplayName(ChatColor.GOLD + "/rec saddle");
			Saddle.setItemMeta(SaddleMeta);
			inv.setItem(recipeIndex++, Saddle);
		}
		if (getConfig().getBoolean("Chain Helmet")== true){
			ItemStack chainhelmet = new ItemStack(Material.CHAINMAIL_HELMET);
			ItemMeta chainhelmetMeta = chainhelmet.getItemMeta();
			chainhelmetMeta.setDisplayName(ChatColor.GOLD + "/rec chainhelmet");
			chainhelmet.setItemMeta(chainhelmetMeta);
			inv.setItem(recipeIndex++, chainhelmet);
		}
		if (getConfig().getBoolean("Chain Leggings")== true){
			ItemStack Saddle = new ItemStack(Material.CHAINMAIL_CHESTPLATE);
			ItemMeta SaddleMeta = Saddle.getItemMeta();
			SaddleMeta.setDisplayName(ChatColor.GOLD + "/rec chainleggings");
			Saddle.setItemMeta(SaddleMeta);
			inv.setItem(recipeIndex++, Saddle);
		}
		if (getConfig().getBoolean("Chain Chestplate")== true){
			ItemStack chainchest = new ItemStack(Material.CHAINMAIL_LEGGINGS);
			ItemMeta chainchestMeta = chainchest.getItemMeta();
			chainchestMeta.setDisplayName(ChatColor.GOLD + "/rec chainchest");
			chainchest.setItemMeta(chainchestMeta);
			inv.setItem(recipeIndex++, chainchest);
		}
		
		if (getConfig().getBoolean("Chain Boots")== true){
			ItemStack chainboots = new ItemStack(Material.CHAINMAIL_BOOTS);
			ItemMeta chainbootsMeta = chainboots.getItemMeta();
			chainbootsMeta.setDisplayName(ChatColor.GOLD + "/rec chainboots");
			chainboots.setItemMeta(chainbootsMeta);
			inv.setItem(recipeIndex++, chainboots);
		}
		
		commander.openInventory(inv);
	}

	@EventHandler
	public void onInventoryClick(InventoryClickEvent event) {
		if (!ChatColor.stripColor(event.getInventory().getName())
				.equalsIgnoreCase("Recipes list - Click for info!"))
			return;
		Player commander = (Player) event.getWhoClicked();
		event.setCancelled(true);

		if (event.getCurrentItem() == null
				|| event.getCurrentItem().getType() == Material.AIR
				|| !event.getCurrentItem().hasItemMeta())
		{
			commander.closeInventory();
			return;
		}
		switch (event.getCurrentItem().getType())
		{
			case GOLD_RECORD:
				openGui1(((Player) commander).getPlayer());

				break;
								
			case GREEN_RECORD:
				openGui2(((Player) commander).getPlayer());
				break;
			case RECORD_3:
				openGui3(((Player) commander).getPlayer());
				break;
				
			case RECORD_4:
				openGui4(((Player) commander).getPlayer());
				break;
			case RECORD_5:
				openGui5(((Player) commander).getPlayer());
				break;
			case RECORD_6:
				openGui6(((Player) commander).getPlayer());
				break;
			case RECORD_7:
				openGui7(((Player) commander).getPlayer());
				break;
			case RECORD_8:
				openGui8(((Player) commander).getPlayer());
				break;
			case RECORD_9:
				openGui9(((Player) commander).getPlayer());
				break;
			case RECORD_10:
				openGui10(((Player) commander).getPlayer());
				break;
			case RECORD_11:
				openGui11(((Player) commander).getPlayer());
				break;
			case RECORD_12:
				openGui12(((Player) commander).getPlayer());
				break;
			case GOLD_BARDING:
				openGui13(((Player) commander).getPlayer());
				break;
			case IRON_BARDING:
				openGui14(((Player) commander).getPlayer());
				break;
			case DIAMOND_BARDING:
				openGui15(((Player) commander).getPlayer());
				break;
			case SADDLE:
				openGui16(((Player) commander).getPlayer());
				break;
			case CHAINMAIL_HELMET:
				openGui17(((Player) commander).getPlayer());
				break;
			case CHAINMAIL_CHESTPLATE:
				openGui18(((Player) commander).getPlayer());
				break;
			case CHAINMAIL_LEGGINGS:
				openGui19(((Player) commander).getPlayer());
				break;
			case CHAINMAIL_BOOTS:
				openGui20(((Player) commander).getPlayer());
				break;
			default:
				commander.closeInventory();
				break;
		}
	}
	
	
	
	private void openGui1(Player commander) {

		Inventory inv2 = Bukkit.createInventory(null, 45, ChatColor.DARK_BLUE
				+ "Showing recipe for Disc 13"); 
		
		ItemStack Coal1 = new ItemStack(Material.COAL);
		ItemMeta Coal1Meta = Coal1.getItemMeta();
		Coal1Meta.setDisplayName("coal");
		Coal1.setItemMeta(Coal1Meta);
		inv2.setItem(10, Coal1);
		ItemStack Coal2 = new ItemStack(Material.COAL);
		ItemMeta Coal2Meta = Coal2.getItemMeta();
		Coal2Meta.setDisplayName("coal");
		Coal2.setItemMeta(Coal2Meta);
		inv2.setItem(11, Coal2);
		ItemStack Coal3 = new ItemStack(Material.COAL);
		ItemMeta Coal3Meta = Coal3.getItemMeta();
		Coal3Meta.setDisplayName("coal");
		Coal3.setItemMeta(Coal3Meta);
		inv2.setItem(12, Coal3);
		ItemStack Coal4 = new ItemStack(Material.COAL);
		ItemMeta Coal4Meta = Coal4.getItemMeta();
		Coal4Meta.setDisplayName("coal");
		Coal4.setItemMeta(Coal4Meta);
		inv2.setItem(19, Coal4);
		ItemStack Gold = new ItemStack(Material.GOLD_INGOT);
		ItemMeta GoldMeta = Gold.getItemMeta();
		GoldMeta.setDisplayName("Gold Ingot");
		Gold.setItemMeta(GoldMeta);
		inv2.setItem(20, Gold);
		ItemStack Coal5 = new ItemStack(Material.COAL);
		ItemMeta Coal5Meta = Coal5.getItemMeta();
		Coal5Meta.setDisplayName("coal");
		Coal5.setItemMeta(Coal5Meta);
		inv2.setItem(21, Coal5);
		ItemStack Coal6 = new ItemStack(Material.COAL);
		ItemMeta Coal6Meta = Coal6.getItemMeta();
		Coal6Meta.setDisplayName("coal");
		Coal6.setItemMeta(Coal6Meta);
		inv2.setItem(28, Coal6);
		ItemStack Coal7 = new ItemStack(Material.COAL);
		ItemMeta Coal7Meta = Coal7.getItemMeta();
		Coal7Meta.setDisplayName("coal");
		Coal7.setItemMeta(Coal7Meta);
		inv2.setItem(29, Coal7);
		ItemStack Coal8 = new ItemStack(Material.COAL);
		ItemMeta Coal8Meta = Coal8.getItemMeta();
		Coal8Meta.setDisplayName("coal");
		Coal8.setItemMeta(Coal8Meta);
		inv2.setItem(30, Coal8);
		ItemStack Back = new ItemStack(Material.REDSTONE_BLOCK);
		ItemMeta BackMeta = Back.getItemMeta();
		BackMeta.setDisplayName(ChatColor.BLUE + "Go Back");
		Back.setItemMeta(BackMeta);
		inv2.setItem(44, Back);
		ItemStack Disc13 = new ItemStack(Material.GOLD_RECORD);
		ItemMeta Disc13Meta = Disc13.getItemMeta();
		Disc13Meta.setDisplayName(ChatColor.AQUA + "Music Disc");
		Disc13.setItemMeta(Disc13Meta);
		inv2.setItem(24, Disc13);
		commander.openInventory(inv2);
	}
	private void openGui2(Player commander) {

		Inventory inv3 = Bukkit.createInventory(null, 45, ChatColor.DARK_BLUE
				+ "Showing recipe for Disc cat"); 
		
		ItemStack Coal1 = new ItemStack(Material.COAL);
		ItemMeta Coal1Meta = Coal1.getItemMeta();
		Coal1Meta.setDisplayName("coal");
		Coal1.setItemMeta(Coal1Meta);
		inv3.setItem(10, Coal1);
		ItemStack Coal2 = new ItemStack(Material.COAL);
		ItemMeta Coal2Meta = Coal2.getItemMeta();
