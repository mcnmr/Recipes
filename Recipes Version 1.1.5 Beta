import org.bukkit.inventory.meta.ItemMeta;
import org.bukkit.plugin.PluginDescriptionFile;
import org.bukkit.plugin.java.JavaPlugin;
import org.bukkit.ChatColor;
import org.bukkit.command.Command;
import org.bukkit.command.CommandSender;
import org.bukkit.configuration.file.FileConfiguration;
import org.bukkit.entity.Player;
import org.bukkit.event.EventHandler;
import org.bukkit.event.Listener;
import org.bukkit.event.inventory.InventoryClickEvent;

public class Main extends JavaPlugin implements Listener
{
	public static Main plugin;
	public final Logger logger = Logger.getLogger("Minecraft");
	int counter = 0;

	public void onDisable() {
		PluginDescriptionFile pdfFile = this.getDescription();
		this.logger.info(pdfFile.getName() + " Has been disabled!");
		getServer().clearRecipes();
	}

	public void onEnable() {
		FileConfiguration config = getConfig();
		
		if (config.getBoolean("Disc 13") == true)
		{
			getServer().getPluginManager().registerEvents(this, this);
			ShapedRecipe disk = new ShapedRecipe(new ItemStack(
				Material.GOLD_RECORD, 1));
			disk.shape(new String[] { "$$$", "$*$", "$$$" })
			.setIngredient('$', Material.COAL)
			.setIngredient('*', Material.GOLD_INGOT);
			getServer().addRecipe(disk);
		}
		
		if (config.getBoolean("Disc cat") == true);
		{
			ShapedRecipe disk2 = new ShapedRecipe(new ItemStack(
					Material.GREEN_RECORD, 1));
			disk2.shape(new String[] { "$$$", "$1$", "$$$" })
					.setIngredient('$', Material.COAL)
					.setIngredient('1', Material.SUGAR_CANE);
			getServer().addRecipe(disk2);
		}
		if (config.getBoolean("Disc blocks") == true);
		{
			ShapedRecipe disk3 = new ShapedRecipe(new ItemStack(Material.RECORD_3,
					1));
			disk3.shape(new String[] { "$$$", "$2$", "$$$" })
					.setIngredient('$', Material.COAL)
					.setIngredient('2', Material.BRICK);
			getServer().addRecipe(disk3);
		}
		if (config.getBoolean("Disc chirp") == true);
		{
			ShapedRecipe disk4 = new ShapedRecipe(new ItemStack(Material.RECORD_4,
					1));
			disk4.shape(new String[] { "$$$", "$3$", "$$$" })
					.setIngredient('$', Material.COAL)
					.setIngredient('3', Material.REDSTONE);
			getServer().addRecipe(disk4);
		}
		if (config.getBoolean("Disc far") == true);
		{
		ShapedRecipe disk5 = new ShapedRecipe(new ItemStack(Material.RECORD_5,
				1));
		disk5.shape(new String[] { "$$$", "$4$", "$$$" })
				.setIngredient('$', Material.COAL)
				.setIngredient('4', Material.CACTUS);
		getServer().addRecipe(disk5);
		}
		if (config.getBoolean("Disc mall") == true);
		{
		ShapedRecipe disk6 = new ShapedRecipe(new ItemStack(Material.RECORD_6,
				1));
		disk6.shape(new String[] { "$$$", "$5$", "$$$" })
				.setIngredient('$', Material.COAL)
				.setIngredient('5', Material.LAPIS_BLOCK);
		getServer().addRecipe(disk6);
