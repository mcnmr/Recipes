	public boolean onCommand(CommandSender sender, Command cmd,
			String commandLabel, String[] args) {
		if (args.length > 0)
		{
		
		}
		if (sender instanceof Player)
		{
			Player commander = (Player) sender;
			if (commandLabel.equalsIgnoreCase("rec"))
			{
				if (args.length == 0)
				{
					commander.sendMessage(ChatColor.DARK_AQUA
							+ "--------------" + ChatColor.GREEN
							+ " Recipes Help " + ChatColor.DARK_AQUA
							+ "--------------");
					commander.sendMessage(ChatColor.DARK_AQUA + "/rec"
							+ ChatColor.WHITE + " | " + ChatColor.GREEN
							+ "Shows this screen.");
					commander.sendMessage(ChatColor.DARK_AQUA + "/rec list"
							+ ChatColor.WHITE + " | " + ChatColor.GREEN
							+ "Shows the items you can craft.");
					commander.sendMessage(ChatColor.DARK_AQUA
							+ "/rec <Item name> " + ChatColor.WHITE + " | "
							+ ChatColor.GREEN
							+ "Will show you how to craft the item specified.");
					commander.sendMessage(ChatColor.DARK_AQUA + "/rec info"
							+ ChatColor.WHITE + " | " + ChatColor.GREEN
							+ "Shows info about the author of this plugin!");
					commander.sendMessage(ChatColor.DARK_AQUA
							+ "--------------" + ChatColor.GREEN
							+ " Recipes Help " + ChatColor.DARK_AQUA
							+ "--------------");
				}

				else if (args[0].equalsIgnoreCase("list"))
				{
					openGUI(((Player) commander).getPlayer());
				}
				else if (args[0].equalsIgnoreCase("13"))
				{
					if (args.length == 1)
					{
						openGui1(((Player) commander).getPlayer());
					}
				}
				else if (args[0].equalsIgnoreCase("cat"))
				{
					if (args.length == 1)
					{
						openGui2(((Player) commander).getPlayer());
					}
				}
				else if (args[0].equalsIgnoreCase("blocks"))
				{
					if (args.length == 1)
					{
						openGui3(((Player) commander).getPlayer());
					}
				}
				else if (args[0].equalsIgnoreCase("Chirp"))
				{
					openGui4(((Player) commander).getPlayer());
				}
				else if (args[0].equalsIgnoreCase("far"))
				{
					if (args.length == 1)
					{
						openGui5(((Player) commander).getPlayer());
					}
				}
				else if (args[0].equalsIgnoreCase("mall"))
				{
					if (args.length == 1)
					{
						openGui6(((Player) commander).getPlayer());
					}
				}
				else if (args[0].equalsIgnoreCase("mellohi"))
				{
					if (args.length == 1)
					{
						openGui7(((Player) commander).getPlayer());
					}
				}
				else if (args[0].equalsIgnoreCase("stal"))
				{
					if (args.length == 1)
					{
						openGui8(((Player) commander).getPlayer());
					}
				}
				else if (args[0].equalsIgnoreCase("strad"))
				{
					if (args.length == 1)
					{
						openGui9(((Player) commander).getPlayer());
					}
				}
				else if (args[0].equalsIgnoreCase("ward"))
				{
					if (args.length == 1)
					{
						openGui10(((Player) commander).getPlayer());
					}
				}
				else if (args[0].equalsIgnoreCase("11"))
				{
					if (args.length == 1)
					{
						openGui11(((Player) commander).getPlayer());
					}
				}
				else if (args[0].equalsIgnoreCase("wait"))
				{
					if (args.length == 1)
					{
						openGui12(((Player) commander).getPlayer());

					}
				}
				else if (args[0].equalsIgnoreCase("goldhorse"))
				{
					if (args.length == 1)
					{
						commander.sendMessage(ChatColor.DARK_AQUA
								+ "--------------" + ChatColor.GREEN
								+ "  Recipes Help " + ChatColor.DARK_AQUA
								+ "--------------");
						commander.sendMessage(ChatColor.DARK_PURPLE
								+ "/rec goldhorse" + ChatColor.WHITE + " | "
								+ ChatColor.GREEN + "Shows this screen.");
						commander.sendMessage(ChatColor.BLUE
								+ "You will need 5 gold ingots and 3 leather.");
						commander.sendMessage(ChatColor.GOLD + " - G G ");
						commander.sendMessage(ChatColor.GOLD + " G G L");
						commander.sendMessage(ChatColor.GOLD + " G L L ");
						commander.sendMessage(ChatColor.BLUE
								+ "G = Gold Ingot, L = Leather");
						commander.sendMessage(ChatColor.DARK_AQUA
								+ "--------------" + ChatColor.GREEN
								+ " Recipes Help " + ChatColor.DARK_AQUA
								+ "--------------");
					}
				}
				else if (args[0].equalsIgnoreCase("ironhorse"))
				{
					if (args.length == 1)
					{
						commander.sendMessage(ChatColor.DARK_AQUA
								+ "--------------" + ChatColor.GREEN
								+ "  Recipes Help " + ChatColor.DARK_AQUA
								+ "--------------");
						commander.sendMessage(ChatColor.DARK_PURPLE
								+ "/rec ironhorse" + ChatColor.WHITE + " | "
								+ ChatColor.GREEN + "Shows this screen.");
						commander.sendMessage(ChatColor.BLUE
								+ "You will need 5 iron ingots and 3 leather.");
						commander.sendMessage(ChatColor.GOLD + " - I I ");
						commander.sendMessage(ChatColor.GOLD + " I I L");
						commander.sendMessage(ChatColor.GOLD + " I L L ");
						commander.sendMessage(ChatColor.BLUE
								+ "I = Iron Ingot, L = Leather");
						commander.sendMessage(ChatColor.DARK_AQUA
								+ "--------------" + ChatColor.GREEN
								+ " Recipes Help " + ChatColor.DARK_AQUA
								+ "--------------");
					}
				}
				else if (args[0].equalsIgnoreCase("diamondhorse"))
				{
					if (args.length == 1)
					{
						commander.sendMessage(ChatColor.DARK_AQUA
								+ "--------------" + ChatColor.GREEN
								+ "  Recipes Help " + ChatColor.DARK_AQUA
								+ "--------------");
						commander.sendMessage(ChatColor.DARK_PURPLE
								+ "/rec diamondhorse" + ChatColor.WHITE + " | "
								+ ChatColor.GREEN + "Shows this screen.");
						commander.sendMessage(ChatColor.BLUE
								+ "You will need 5 Diamonds and 3 leather.");
						commander.sendMessage(ChatColor.GOLD + " - D D ");
						commander.sendMessage(ChatColor.GOLD + " D D L");
						commander.sendMessage(ChatColor.GOLD + " D L L ");
						commander.sendMessage(ChatColor.BLUE
								+ "D = Diamond, L = Leather");
						commander.sendMessage(ChatColor.DARK_AQUA
								+ "--------------" + ChatColor.GREEN
								+ " Recipes Help " + ChatColor.DARK_AQUA
								+ "--------------");
					}
				}
				else if (args[0].equalsIgnoreCase("saddle"))
				{
					if (args.length == 1)
					{
						commander.sendMessage(ChatColor.DARK_AQUA
								+ "--------------" + ChatColor.GREEN
								+ "  Recipes Help " + ChatColor.DARK_AQUA
								+ "--------------");
						commander.sendMessage(ChatColor.DARK_PURPLE
								+ "/rec saddle" + ChatColor.WHITE + " | "
								+ ChatColor.GREEN + "Shows this screen.");
						commander.sendMessage(ChatColor.BLUE
								+ "You will need 9 Leather.");
						commander.sendMessage(ChatColor.GOLD + " L L L ");
						commander.sendMessage(ChatColor.GOLD + " L L L ");
						commander.sendMessage(ChatColor.GOLD + " L L L ");
						commander.sendMessage(ChatColor.BLUE + " L = Leather");
						commander.sendMessage(ChatColor.DARK_AQUA
								+ "--------------" + ChatColor.GREEN
								+ " Recipes Help " + ChatColor.DARK_AQUA
								+ "--------------");
					}
				}
				else if (args[0].equalsIgnoreCase("info"))
				{
					if (args.length == 1)
					{
						commander.sendMessage(ChatColor.DARK_AQUA
								+ "--------------" + ChatColor.GREEN
								+ "  Recipes Info " + ChatColor.DARK_AQUA
								+ "--------------");
						commander.sendMessage(ChatColor.DARK_PURPLE
								+ "/rec info" + ChatColor.WHITE + " | "
								+ ChatColor.GREEN + "Shows this screen.");
						commander.sendMessage(ChatColor.RED
								+ "Plugin created by mcnmr.");
						commander
								.sendMessage(ChatColor.DARK_AQUA
										+ " This plugin orginally only had a few features until I decided to expand on it. ");
						commander
								.sendMessage(ChatColor.DARK_AQUA
										+ "You can suggest features to me on the my server.");
						commander.sendMessage(ChatColor.DARK_AQUA
								+ "--------------" + ChatColor.GREEN
								+ " Recipes Info " + ChatColor.DARK_AQUA
								+ "--------------");

					}
				}
				else if (args[0].equalsIgnoreCase("help"))
				{
					if (args.length == 1)
					{
						commander.sendMessage(ChatColor.DARK_AQUA
								+ "--------------" + ChatColor.GREEN
								+ " Recipes Help " + ChatColor.DARK_AQUA
								+ "--------------");
						commander.sendMessage(ChatColor.DARK_AQUA + "/rec help"
								+ ChatColor.WHITE + " | " + ChatColor.GREEN
								+ "Shows this screen.");
						commander.sendMessage(ChatColor.DARK_AQUA + "/rec list"
								+ ChatColor.WHITE + " | " + ChatColor.GREEN
								+ "Shows the items you can craft.");
						commander.sendMessage(ChatColor.DARK_AQUA
								+ "/rec <Item name> " + ChatColor.WHITE + " | "
								+ ChatColor.GREEN
								+ "Will show you how to craft the item specified.");
						commander.sendMessage(ChatColor.DARK_AQUA + "/rec info"
								+ ChatColor.WHITE + " | " + ChatColor.GREEN
								+ "Shows info about the author of this plugin!");
						commander.sendMessage(ChatColor.DARK_AQUA
								+ "--------------" + ChatColor.GREEN
								+ " Recipes Help " + ChatColor.DARK_AQUA
								+ "--------------");
					}
				}
				else {
					commander.sendMessage(ChatColor.DARK_RED + "Unknown command please type /rec help for a list of commands!");
				}
					
			}
		}
		return false;
	}
}

